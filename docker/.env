# Docker project name
COMPOSE_PROJECT_NAME=musefully

# Version of Elastic stack
STACK_VERSION=8.17.0

# Set to 'basic' or 'trial' to automatically start the 30-day trial
LICENSE=basic

# Port to expose Elasticsearch HTTP API to the host
ES_PORT=9200

# Port to expose Kibana to the host
KIBANA_PORT=5601

# Increase or decrease based on the available host memory (in bytes) (default 1g)
MEM_LIMIT=1g